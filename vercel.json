{
  "buildCommand": "mkdir -p dist && cp *.html *.txt dist/",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/send-scheduled-notifications",
      "schedule": "*/15 * * * *"
    }
  ]
}
